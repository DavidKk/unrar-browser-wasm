{
  "name": "unrar-browser-wasm",
  "version": "1.0.0",
  "description": "WebAssembly build of UnRAR library",
  "scripts": {
    "commit": "git-cz",
    "ci:pages:init": "shx rm -rf ./gh-pages && shx mkdir -p ./gh-pages",
    "ci:pages:build": "pnpm build && pnpm build:demos",
    "ci:pages:copy:unrar": "shx cp packages/unrar-wasm/build/unrar.js packages/unrar-wasm/build/unrar.wasm ./gh-pages/",
    "ci:pages:copy:vite-demo": "shx mkdir -p ./gh-pages/vite-demo && shx cp -r packages/vite-demo/dist/* ./gh-pages/vite-demo/",
    "ci:pages:copy:nextjs-demo": "shx mkdir -p ./gh-pages/nextjs-demo && shx cp -r packages/nextjs-demo/out/* ./gh-pages/nextjs-demo/",
    "ci:pages:copy:e2e-demo": "shx mkdir -p ./gh-pages/e2e-demo && shx cp -r packages/e2e-demo/dist/* ./gh-pages/e2e-demo/",
    "ci:pages": "pnpm ci:pages:init && pnpm ci:pages:build && pnpm ci:pages:copy:unrar && pnpm ci:pages:copy:vite-demo && pnpm ci:pages:copy:nextjs-demo && pnpm ci:pages:copy:e2e-demo",
    "ci:coverage": "pnpm build && pnpm test:e2e --project=chromium",
    "ci:build": "pnpm build",
    "postinstall": "node --experimental-modules ./scripts/postinstall.mjs",
    "preinstall": "node --experimental-modules ./scripts/preinstall.mjs",
    "format": "prettier --config .prettierrc.js --write \"**/*.{js,jsx,ts,tsx,d.ts,vue,md,json,yml,yaml}\"",
    "lint": "eslint -c eslint.config.mjs --fix .",
    "build": "pnpm --filter @unrar-browser/core build",
    "prepare:e2e": "shx mkdir -p packages/e2e-demo/public && shx cp packages/unrar-wasm/build/unrar.js packages/unrar-wasm/build/unrar.wasm packages/e2e-demo/public/",
    "build:demos": "pnpm prepare:e2e && pnpm --filter @unrar-browser/vite-demo build && pnpm --filter @unrar-browser/e2e-demo build && pnpm --filter @unrar-browser/nextjs-demo build",
    "dev": "pnpm --filter @unrar-browser/vite-demo dev",
    "predev:e2e": "pnpm run prepare:e2e || true",
    "dev:e2e": "pnpm --filter @unrar-browser/e2e-demo dev",
    "dev:node": "pnpm --filter @unrar-browser/node-demo dev",
    "dev:nextjs": "pnpm --filter @unrar-browser/nextjs-demo dev",
    "node-demo": "pnpm --filter @unrar-browser/node-demo start",
    "test": "pnpm test:unit && pnpm test:web && pnpm test:e2e",
    "test:unit": "jest --config jest.config.unittest.ts",
    "test:web": "jest --config jest.config.webtest.ts",
    "pretest:e2e": "pnpm run prepare:e2e || true",
    "test:e2e": "playwright test --reporter=list",
    "ok": "pnpm lint && pnpm build && pnpm test:e2e",
    "ci": "pnpm lint && pnpm build && pnpm test:e2e --project=chromium"
  },
  "keywords": [
    "wasm",
    "unrar",
    "compression"
  ],
  "author": "DavidJones",
  "license": "MIT",
  "dependencies": {
    "json5": "^2.2.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/eslintrc": "^3",
    "@jest/types": "^29.6.3",
    "@playwright/test": "^1.48.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/chalk": "^2.2.0",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "@vitejs/plugin-react": "^5.0.3",
    "canvas": "^3.2.0",
    "chalk": "^5.3.0",
    "commitizen": "^4.2.4",
    "commitlint-plugin-i18n": "^0.0.1-alpha.3",
    "cz-customizable": "^7.2.1",
    "cz-customizable-i18n-config": "^0.0.1-alpha.2",
    "eslint": "^9",
    "eslint-config-next": "15.1.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.1",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^30.1.2",
    "jest-junit": "^16.0.0",
    "jest-runner-tsd": "^6.0.0",
    "json5": "^2.2.3",
    "lint-staged": "^15.2.7",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "pretty-error": "^4.0.0",
    "rimraf": "^6.0.1",
    "shx": "^0.3.4",
    "spinnies": "^0.5.1",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "tsd-lite": "^0.9.0",
    "typescript": "^5.7.2",
    "typescript-transform-paths": "^3.4.7",
    "vite": "^7.1.6"
  },
  "packageManager": "pnpm@10.2.0+sha512.0d27364e0139c6aadeed65ada153135e0ca96c8da42123bd50047f961339dc7a758fc2e944b428f52be570d1bd3372455c1c65fa2e7aa0bfbf931190f9552001"
}
