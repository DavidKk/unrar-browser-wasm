{
  "name": "@unrar-browser/core",
  "version": "1.0.1",
  "description": "WebAssembly build of UnRAR library for browser",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build:wasm": "shx rm -rf build && shx mkdir -p build && cd build && emcmake cmake .. && emmake make",
    "build:ts": "tsc",
    "build": "pnpm run build:wasm && pnpm run build:ts",
    "clean": "shx rm -rf build dist",
    "lint:ts": "eslint --fix ."
  },
  "devDependencies": {
    "shx": "^0.3.4",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "unrar",
    "wasm",
    "webassembly",
    "rar",
    "extract",
    "browser",
    "decompression"
  ],
  "author": "DavidJones <qowera@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DavidKk/unrar-browser-wasm.git",
    "directory": "packages/unrar-wasm"
  },
  "bugs": {
    "url": "https://github.com/DavidKk/unrar-browser-wasm/issues"
  },
  "homepage": "https://github.com/DavidKk/unrar-browser-wasm#readme",
  "files": [
    "dist/",
    "build/unrar.js",
    "build/unrar.wasm",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
